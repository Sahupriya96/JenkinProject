pipeline {
    agent any

    stages 
    {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
     
        stage('Hell') {
            steps {
                echo 'Hell World'
            }
        }
    
     
        stage('Hi') {
            steps {
                echo 'Hi World'
            }
        }
    }
    
}
